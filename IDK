local debugTab = Window:CreateTab("Devloper", 4483362458)

local debugSection = debugTab:CreateSection("debug function")

    local Button = debugTab:CreateButton({
        Name = "Create Coordinates Menu",
        Callback = function()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
    
            local screenGui = Instance.new("ScreenGui")
            screenGui.Name = "CoordinatesGui"
            screenGui.Parent = player:WaitForChild("PlayerGui")
    
            local textLabel = Instance.new("TextLabel")
            textLabel.Size = UDim2.new(0, 300, 0, 50)
            textLabel.Position = UDim2.new(0, 10, 0, 10)
            textLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            textLabel.BackgroundTransparency = 0.5
            textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            textLabel.TextSize = 20
            textLabel.Parent = screenGui
    
            local function showCoordinates()
                local position = character:WaitForChild("HumanoidRootPart").Position
                textLabel.Text = "X: " .. math.floor(position.X) .. " Y: " .. math.floor(position.Y) .. " Z: " .. math.floor(position.Z)
            end
    
            game:GetService("RunService").Heartbeat:Connect(function()
                showCoordinates()
            end)
        end,
    })

local Button = debugTab:CreateButton({
    Name = "Remove Coordinates Menu",
    Callback = function()
        local player = game.Players.LocalPlayer
        local screenGui = player:FindFirstChild("PlayerGui"):FindFirstChild("CoordinatesGui")
        
        if screenGui then
            screenGui:Destroy()
        end
    end,
})

debugTab:CreateButton({
    Name = "Safe Zone",
    Callback = function()
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local Workspace = game:GetService("Workspace")

        local safeZone = Workspace:FindFirstChild("Safe Zone")

        if not safeZone then
            safeZone = Instance.new("Part")
            safeZone.Name = "Safe Zone"
            safeZone.Size = Vector3.new(100, 1, 100) 
            safeZone.Position = Vector3.new(10000, 50, 10000)
            safeZone.Anchored = true
            safeZone.CanCollide = true
            safeZone.Parent = Workspace

            print("Safe Zone platform created at: " .. tostring(safeZone.Position))
        else
            print("Safe Zone platform already exists.")
        end

        if LocalPlayer and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(safeZone.Position + Vector3.new(0, 3, 0))
            print("Teleporting to: Safe Zone")
        end
    end
})

local Toggle = debugTab:CreateToggle({
    Name = "Toggle Example",
    CurrentValue = false,
    Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
    -- The function that takes place when the toggle is pressed
    -- The variable (Value) is a boolean on whether the toggle is true or false
    end,
 })

local Button = debugTab:CreateButton({
    Name = "Remove All Rayfield-Old Menus",
    Callback = function()
        local coreGui = game:GetService("CoreGui")

        for _, child in ipairs(coreGui:GetChildren()) do
            if child.Name == "Rayfield-Old" then
                child:Destroy()
            end
        end
    end,
})

    player.CharacterAdded:Connect(function(newCharacter)
        if connection then
            connection:Disconnect()
            connection = nil
        end

        if isFollowing then
            connection = game:GetService("RunService").RenderStepped:Connect(followPlayer)
        end
    end)
end
