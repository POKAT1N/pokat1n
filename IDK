local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local player = game.Players.LocalPlayer
local movingPart = nil
local afkZone = game.Workspace.map:FindFirstChild("AFK –∑–æ–Ω–∞")

if afkZone then
    for _, part in ipairs(afkZone:GetChildren()) do
        if part:IsA("Part") and part:FindFirstChildOfClass("Script") then
            movingPart = part
            break
        end
    end
end

if movingPart then
    local originalPosition = movingPart.Position
    local isFollowing = false
    local connection

    local function followPlayer()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            movingPart.Position = player.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0)
        end
    end

local Window = Rayfield:CreateWindow({
    Name = "üî• —Ö–∑ –∫–∞–∫ –Ω–∞–∑–≤–∞—Ç—å | –†–ì–ß üî´",
    LoadingTitle = "üî´ –†—É—Å—Å–∫–∏–π –ì–æ–ª–æ—Å–≤–æ–π –ß–∞—Ç üí•",
    LoadingSubtitle = "by POKAT1N",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "PENIS HUB"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title = "üî• BadW0rld C0MMUNITY | –†–ì–ßüî´",
        Subtitle = "Key System",
        Note = "SCRIPT IN ALPHA",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"TEST"}
    }
})

local HomeTab = Window:CreateTab("Home", 4483362458)

local MainSection = HomeTab:CreateSection("Enter and exit ragdoll mode before using (if you reset your avatar, then repeat it again)")

HomeTab:CreateButton({
    Name = "ANTI RAGDOLL",
    Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/POKAT1N/pokat1n/refs/heads/main/AntiRagdollForRussianVoiceChat'))()
    end
})

local MainSection = HomeTab:CreateSection("Main")

local Toggle = HomeTab:CreateToggle({
    Name = "Collision (Don`t work? rejoin!)",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)

        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer

                if LocalPlayer and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local humanoidRootPart = LocalPlayer.Character.HumanoidRootPart
            
            if Value then
                humanoidRootPart.CollisionGroupId = 6
            else
                humanoidRootPart.CollisionGroupId = 5
            end
        end
    end,
})


    local Toggle = HomeTab:CreateToggle({
        Name = "ForceField",
        CurrentValue = false,
        Flag = "Toggle1",
        Callback = function(Value)
            isFollowing = Value

            if isFollowing then
                connection = game:GetService("RunService").RenderStepped:Connect(followPlayer)
            else
                if connection then
                    connection:Disconnect()
                    connection = nil
                end
                movingPart.Position = originalPosition
            end
        end,
    })
 
