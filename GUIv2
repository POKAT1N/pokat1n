local Toggle = MainTab:CreateButton({
    Name = "PORTAL DELETE",
    Callback = function()
   -- –£–∫–∞–∑—ã–≤–∞–µ–º —Å–ø–∏—Å–æ–∫ –∏–º–µ–Ω –ø–æ—Ä—Ç–∞–ª–æ–≤, –∫–æ—Ç–æ—Ä—ã–µ –Ω—É–∂–Ω–æ —É–¥–∞–ª–∏—Ç—å
local portalNames = {"–ü–æ—Ä—Ç–∞–ª", "–ü–æ—Ä—Ç–∞–ª", "Portal", "Dobro_RussianChatNEW"}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–æ–≤ —Å —É–∫–∞–∑–∞–Ω–Ω—ã–º–∏ –∏–º–µ–Ω–∞–º–∏
local function deletePortals()
    for _, name in ipairs(portalNames) do
        local portal = game.Workspace:FindFirstChild(name)
        if portal then
            portal:Destroy()
            print(name .. " —É–¥–∞–ª–µ–Ω –∏–∑ Workspace.")
        else
            print(name .. " –Ω–µ –Ω–∞–π–¥–µ–Ω.")
        end
    end
end

-- –í—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ —É–¥–∞–ª–µ–Ω–∏—è
deletePortals()

    end,
 })

